    "lint": "eslint \"src/client/**/*.ts*\"",
    "lint:fix": "eslint --fix --cache \"src/client/**/*.ts*\"",
    "less:lint": "stylelint \"src/client/app/**/*.less\"",
    "dev": "cross-env NODE_OPTIONS='--openssl-legacy-provider' NODE_ENV=development node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
